{
  "name": "reacttemplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "React app with server and all that jazz.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Corbineau/reactTemplate.git"
  },
  "keywords": [
    "template"
  ],
  "author": "Rabbit",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "faker": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "browserify": "^16.4.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16"
  },
  "bugs": {
    "url": "https://github.com/Corbineau/reactTemplate/issues"
  },
  "homepage": "https://github.com/Corbineau/reactTemplate#readme"
}
